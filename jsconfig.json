{
  "typeAcquisition": {
    "exclude": []
  },
  "allowJS": true,
  "compilerOptions": {
    "module": "ES6",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"],
      "~~/*": ["./src/*"],
      "@@/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules"]
}
